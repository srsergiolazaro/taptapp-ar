{
    "name": "@taptapp/ar",
    "version": "1.0.0",
    "description": "AR Visualizer and Compiler for Astro and React",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./astro/*": "./src/astro/*.astro",
        "./compiler/*": "./src/compiler/*.js"
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch"
    },
    "peerDependencies": {
        "astro": ">=4.0.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "three": ">=0.160.0",
        "aframe": ">=1.5.0"
    },
    "dependencies": {
        "@tensorflow/tfjs": "^4.22.0",
        "@tensorflow/tfjs-backend-cpu": "^4.22.0",
        "@tensorflow/tfjs-backend-webgl": "^4.22.0",
        "ml-matrix": "^6.10.4",
        "svd-js": "^1.1.1",
        "tinyqueue": "^2.0.3",
        "canvas": "^3.1.0"
    },
    "devDependencies": {
        "typescript": "^5.4.5",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/three": "^0.170.0",
        "astro": "5.16.4"
    }
}