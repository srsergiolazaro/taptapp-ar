{
    "name": "@srsergio/taptapp-ar",
    "version": "1.0.64",
    "author": "Sergio Lazaro <srsergiolazaro@gmail.com>",
    "license": "GPL-3.0",
    "description": "Ultra-fast, lightweight Augmented Reality Image Tracking SDK for the web. Features an optimized offline compiler, React components, and compatibility with Three.js/A-Frame. No heavy ML frameworks required.",
    "keywords": [
        "web-ar",
        "augmented-reality",
        "image-tracking",
        "ar-compiler",
        "computer-vision",
        "three-js",
        "react-ar",
        "a-frame",
        "mind-ar",
        "offline-ar",
        "webgl",
        "markerless-tracking",
        "taptapp"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/srsergiolazaro/taptapp-ar.git"
    },
    "bugs": {
        "url": "https://github.com/srsergiolazaro/taptapp-ar/issues"
    },
    "homepage": "https://github.com/srsergiolazaro/taptapp-ar#readme",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./compiler/*": "./src/compiler/*"
    },
    "files": [
        "dist",
        "src",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "vitest"
    },
    "peerDependencies": {
        "aframe": ">=1.5.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0",
        "three": ">=0.160.0"
    },
    "peerDependenciesMeta": {
        "aframe": {
            "optional": true
        },
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        },
        "three": {
            "optional": true
        }
    },
    "dependencies": {
        "@msgpack/msgpack": "^3.0.0-beta2",
        "ml-matrix": "^6.10.4",
        "tinyqueue": "^2.0.3"
    },
    "devDependencies": {
        "@types/node": "^25.0.3",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@types/three": "^0.170.0",
        "jimp": "^1.6.0",
        "typescript": "^5.4.5",
        "vitest": "^4.0.16"
    },
    "publishConfig": {
        "access": "public"
    }
}
